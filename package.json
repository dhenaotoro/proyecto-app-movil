{
  "name": "proyectoAppMovil",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "ENVFILE=.env react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "clean": "jest --clearCache",
    "start": "react-native start --reset-cache",
    "test": "jest --no-cache --runInBand --detectOpenHandles --config jest.config.js",
    "test-report": "jest --no-cache --reporters=default --reporters=jest-junit --coverage --config jest.config.js",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "android:bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "e2e:build-ios-debug": "detox build -c ios.sim.debug",
    "e2e:build-ios-release": "detox build -c ios.sim.release",
    "e2e:test-ios-debug": "detox test -c ios.sim.debug",
    "e2e:test-ios-release": "detox test -c ios.sim.release",
    "e2e:build-android-debug": "npm run android:clean && detox build -c android.emu.debug",
    "e2e:build-android-release": "npm run android:clean && detox build -c android.emu.release",
    "pipeline:build-release": "cd android && ./gradlew assembleRelease && cd ..",
    "e2e:test-android-debug": "detox test -c android.emu.debug",
    "e2e:test-android-release": "detox test -c android.emu.release"
  },
  "dependencies": {
    "@aws-amplify/react-native": "^1.1.6",
    "@aws-amplify/ui-react-native": "^2.2.14",
    "@babel/plugin-proposal-decorators": "^7.25.7",
    "@babel/plugin-transform-modules-commonjs": "^7.25.7",
    "@babel/plugin-transform-named-capturing-groups-regex": "^7.25.7",
    "@react-native-async-storage/async-storage": "^2.0.0",
    "@react-native-community/checkbox": "^0.5.17",
    "@react-native-community/datetimepicker": "^8.2.0",
    "@react-native-masked-view/masked-view": "^0.3.1",
    "@react-native-picker/picker": "^2.8.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@react-navigation/stack": "^6.4.1",
    "amazon-cognito-identity-js": "^6.3.12",
    "aws-amplify": "^6.6.4",
    "axios": "^1.7.7",
    "jest-junit": "^16.0.0",
    "react": "18.3.1",
    "react-native": "0.75.3",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "^2.20.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-reanimated": "^3.15.4",
    "react-native-safe-area-context": "^4.11.0",
    "react-native-screens": "^3.34.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vector-icons": "^10.2.0",
    "ts-jest": "^29.2.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.7",
    "@babel/preset-env": "^7.25.8",
    "@babel/preset-react": "^7.25.7",
    "@babel/preset-typescript": "^7.25.7",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.75.3",
    "@react-native/eslint-config": "0.75.3",
    "@react-native/metro-config": "0.75.3",
    "@react-native/typescript-config": "0.75.3",
    "@testing-library/react-native": "^12.7.2",
    "@types/jest": "^29.5.13",
    "@types/react": "^18.2.6",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.7.0",
    "detox": "^20.27.3",
    "eslint": "^8.19.0",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "2.8.8",
    "react-test-renderer": "^18.3.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
